-- Table: public.tesla_macd_data

-- DROP TABLE public.tesla_macd_data;

CREATE TABLE public.tesla_macd_data
(
    index integer NOT NULL DEFAULT nextval('tesla_macd_data_index_seq'::regclass),
    "Date" date,
    "Open" numeric,
    "High" numeric,
    "Low" numeric,
    "Close" numeric,
    "Volume" numeric,
    "MACD" numeric,
    "Hist" numeric,
    "MACD Value" numeric,
    CONSTRAINT tesla_macd_data_pkey PRIMARY KEY (index)
)

TABLESPACE pg_default;

ALTER TABLE public.tesla_macd_data
    OWNER to postgres;
    
##########################################################

-- Table: public.tesla_macd_results

-- DROP TABLE public.tesla_macd_results;

CREATE TABLE public.tesla_macd_results
(
    index integer NOT NULL DEFAULT nextval('tesla_macd_results_index_seq'::regclass),
    "Date" date,
    "Close" numeric,
    "Hist" numeric,
    "Buy" boolean,
    "Sell" boolean,
    capital numeric,
    shares numeric,
    CONSTRAINT tesla_macd_results_pkey PRIMARY KEY (index)
)

TABLESPACE pg_default;

ALTER TABLE public.tesla_macd_results
    OWNER to postgres;
    
#########################################################

